<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SettingActivity">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/shape_heading_dashboard_gradient" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Settings "
        android:textColor="#000000"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.32"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.506" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView14"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toTopOf="@+id/textView14"
        app:srcCompat="@drawable/setting" />

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="110dp"
                android:layout_height="120dp"
                android:backgroundTint="@color/transparent"
                android:gravity="center"
                android:orientation="vertical"
                app:cardCornerRadius="20dp">

                <androidx.cardview.widget.CardView
                    android:id="@+id/contact"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:backgroundTint="@color/white"
                    android:elevation="15dp"
                    app:cardCornerRadius="20dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="15dp"
                        android:src="@drawable/notifi"
                        app:tint="@color/col1_100" />
                </androidx.cardview.widget.CardView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:gravity="center"
                    android:text="Notification"
                    android:textColor="@color/black"
                    android:textSize="11sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="110dp"
                android:layout_height="120dp"
                android:backgroundTint="@color/transparent"
                android:gravity="center"
                android:orientation="vertical"
                app:cardCornerRadius="20dp">

                <androidx.cardview.widget.CardView
                    android:id="@+id/format"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:backgroundTint="@color/white"
                    android:elevation="15dp"
                    app:cardCornerRadius="20dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="15dp"
                        android:src="@drawable/rrefrewardpng"
                        app:tint="@color/col1_100" />
                </androidx.cardview.widget.CardView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:gravity="center"
                    android:text="Referral rewards"
                    android:textColor="@color/black"
                    android:textSize="11sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="110dp"
                android:layout_height="120dp"
                android:backgroundTint="@color/transparent"
                android:gravity="center"
                android:orientation="vertical"
                app:cardCornerRadius="20dp">

                <androidx.cardview.widget.CardView
                    android:id="@+id/usb"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:backgroundTint="@color/white"
                    android:elevation="15dp"
                    app:cardCornerRadius="20dp">

                    <ImageView
                        android:id="@+id/stopDataTransfer"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="15dp"
                        android:src="@drawable/update"
                        app:tint="@color/col1_100" />
                </androidx.cardview.widget.CardView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:gravity="center"
                    android:text="Check for Updates"
                    android:textColor="@color/black"
                    android:textSize="11sp"
                    android:textStyle="bold" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/privacy"
                android:layout_width="110dp"
                android:layout_height="120dp"
                android:backgroundTint="@color/transparent"
                android:gravity="center"
                android:orientation="vertical"
                app:cardCornerRadius="20dp">

                <androidx.cardview.widget.CardView
                    android:id="@+id/apphide"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:backgroundTint="@color/white"
                    android:elevation="15dp"
                    app:cardCornerRadius="20dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="15dp"
                        android:src="@drawable/tandc"
                        app:tint="@color/col1_100" />
                </androidx.cardview.widget.CardView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:gravity="center"
                    android:text="Terms and Conditions"
                    android:textColor="@color/black"
                    android:textSize="11sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="110dp"
                android:layout_height="120dp"
                android:backgroundTint="@color/transparent"
                android:gravity="center"
                android:orientation="vertical"
                app:cardCornerRadius="20dp">

                <androidx.cardview.widget.CardView
                    android:id="@+id/applock"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:backgroundTint="@color/white"
                    android:elevation="15dp"
                    app:cardCornerRadius="20dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="15dp"
                        android:src="@drawable/privacy"
                        app:tint="@color/col1_100" />
                </androidx.cardview.widget.CardView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:gravity="center"
                    android:text="Privacy Policy"
                    android:textColor="@color/black"
                    android:textSize="11sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="110dp"
                android:layout_height="120dp"
                android:backgroundTint="@color/transparent"
                android:gravity="center"
                android:orientation="vertical"
                app:cardCornerRadius="20dp">

                <androidx.cardview.widget.CardView
                    android:id="@+id/refer"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:backgroundTint="@color/white"
                    android:elevation="15dp"
                    app:cardCornerRadius="20dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="15dp"
                        android:src="@drawable/rate"
                        app:tint="@color/col1_100" />
                </androidx.cardview.widget.CardView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:gravity="center"
                    android:text="Rate App in Playstore"
                    android:textColor="@color/black"
                    android:textSize="11sp"
                    android:textStyle="bold" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="110dp"
                android:layout_height="120dp"
                android:backgroundTint="@color/transparent"
                android:gravity="center"
                android:orientation="vertical"
                app:cardCornerRadius="20dp">

                <androidx.cardview.widget.CardView
                    android:id="@+id/subcription"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:backgroundTint="@color/white"
                    android:elevation="15dp"
                    app:cardCornerRadius="20dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="15dp"
                        android:src="@drawable/help"
                        app:tint="@color/col1_100" />
                </androidx.cardview.widget.CardView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:gravity="center"
                    android:text="Help"
                    android:textColor="@color/black"
                    android:textSize="11sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="110dp"
                android:layout_height="120dp"
                android:backgroundTint="@color/transparent"
                android:gravity="center"
                android:orientation="vertical"
                app:cardCornerRadius="20dp">

                <androidx.cardview.widget.CardView
                    android:id="@+id/referrals"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:backgroundTint="@color/white"
                    android:elevation="15dp"
                    app:cardCornerRadius="20dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="15dp"
                        android:src="@drawable/logout"
                        app:tint="@color/col1_100" />
                </androidx.cardview.widget.CardView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:gravity="center"
                    android:text="Logout"
                    android:textColor="@color/black"
                    android:textSize="11sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="110dp"
                android:layout_height="120dp"
                android:backgroundTint="@color/transparent"
                android:gravity="center"
                android:orientation="vertical"
                app:cardCornerRadius="20dp">

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="200dp"
        android:layout_height="60dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5"
        app:srcCompat="@drawable/getitongoogleplay"
        />

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="81dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView13">

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            app:srcCompat="@drawable/instagram" />

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            app:srcCompat="@drawable/facebook" />

        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            app:srcCompat="@drawable/whatsapp" />

        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            app:srcCompat="@drawable/twitter" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>